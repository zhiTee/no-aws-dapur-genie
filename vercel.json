{
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "GOOGLE_API_KEY": "@google-api-key",
    "ASSEMBLYAI_API_KEY": "@assemblyai-api-key",
